{
  "name": "eslint-plugin-coffeescript",
  "version": "0.3.0",
  "description": "Transpiles coffee files (.coffee, .cjsx) and runs through eslint. Reports errors with original line numbers.",
  "main": "dist/index.js",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "alex <alex@rocketguys.com>",
  "scripts": {
    "test": "ava",
    "build": "node_modules/.bin/babel --out-dir=dist src",
    "prepare": "npm run build"
  },
  "dependencies": {
    "coffeescript": "^2.1.1",
    "eslint-module-utils": "^2.1.1",
    "source-map": "^0.6.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "~3.9.1"
  },
  "repository": "git@github.com:a-x-/eslint-plugin-coffeescript.git",
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT"
}
