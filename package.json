{
  "name": "eslint-plugin-coffeescript",
  "version": "0.3.0",
  "description": "Transpiles coffee files (.coffee, .cjsx) and runs through eslint. Reports errors with original line numbers.",
  "main": "dist/index.js",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "alex <alex@rocketguys.com>",
  "scripts": {
    "test": "node --experimental-modules ./vowsrunner.js test/test_*.coffee --spec",
    "build": "node_modules/.bin/coffee --bare --no-header -t -o dist src",
    "prepare": "npm run build"
  },
  "dependencies": {
    "coffeescript": "^2.1.1",
    "eslint-module-utils": "^2.1.1",
    "source-map": "^0.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^3.9.1",
    "vows": "^0.8.1"
  },
  "repository": "git@github.com:a-x-/eslint-plugin-coffeescript.git",
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT"
}
